{
	"name": "cepharum/contao-cors",
	"description": "Contao 4.x bundle adding CORS support",
	"version": "0.0.1",
	"keywords": [
		"contao",
		"bundle"
	],
	"type": "contao-bundle",
	"homepage": "https://cepharum.de",
	"license": "MIT",
	"authors": [
		{
			"name": "Thomas Urban",
			"homepage": "https://cepharum.de",
			"role": "developer"
		}
	],
	"support": {
		"source": "https://github.com/cepharum/contao-cors",
		"issues": "https://github.com/cepharum/contao-cors/issues"
	},
	"require": {
		"php": ">=7.1",
		"contao/core-bundle": "~4.4"
	},
	"require-dev": {
		"contao/manager-plugin": "^2.0"
	},
	"conflict": {
		"contao/manager-plugin": "<2.0 || >=3.0"
	},
	"autoload": {
		"psr-4": {
			"Cepharum\\Contao\\CorsBundle\\": "src/"
		},
		"classmap": [
			"src/Resources/contao/"
		],
		"exclude-from-classmap": [
			"src/Resources/contao/config/",
			"src/Resources/contao/dca/",
			"src/Resources/contao/languages/",
			"src/Resources/contao/templates/"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Cepharum\\Contao\\CorsBundle\\": "test/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.x-dev"
		},
		"contao": {
			"transifex": {
				"project": "contao-cors",
				"prefix": "master-",
				"languages_cto": "src/Resources/contao/languages",
				"languages_tx": ".tx"
			}
		},
		"contao-manager-plugin": "Cepharum\\Contao\\CorsBundle\\ContaoManager\\Plugin"
	}
}
